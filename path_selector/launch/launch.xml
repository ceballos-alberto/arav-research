<launch>
	
     <!-- Parameters -->
	
	<arg name = "ground_factor" default = "0.8"/> 
	<arg name = "aerial_factor" default = "2"/> 

	<!-- ARAV Path Planning node -->
		
	<node name="path_selector" pkg="path_selector" type="selector.py" output="screen"
     args = "$(arg ground_factor) $(arg aerial_factor)"/>
			
</launch>
	
